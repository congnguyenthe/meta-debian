# versatilepb machine needs ARM EABI support
CONFIG_AEABI=y

# QEMU harddisk
CONFIG_PCI=y
CONFIG_SCSI=y
CONFIG_BLK_DEV_SD=y
CONFIG_SCSI_LOWLEVEL=y
CONFIG_SCSI_SYM53C8XX_2=y

# the default filesystem of QEMU machines
CONFIG_EXT3_FS=y

# tiny-init depends on devtmpfs
CONFIG_DEVTMPFS=y
CONFIG_DEVTMPFS_MOUNT=n
